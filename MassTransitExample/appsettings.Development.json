{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "EventBus": {
    "ConnectionString": "rabbitmq://admin:root1234@localhost:5672"
  },
  "RabbitMqConfig": {
    "UseTls": "true",
    "RabbitMqRootUriTLS": "", // tls (normally 5671 is tls)
    "RabbitMqRootUri": "", // non tls (normally 5672 is non-tls)
    "ServerCertCommonName": "",
    "ClientCertPath": "Properties/certificates/client_certificate.pem",
    "ClientCertPassword": "",
    "UserName": "",
    "Password": ""
  }
}