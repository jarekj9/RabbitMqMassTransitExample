{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "EventBus": {
    "ConnectionString": "rabbitmq://admin:root1234@localhost:5672"
  },
  "RabbitMqTlsConfig": {
    "UseTls": "true",
    "RabbitMqRootUri": "amqps://<your-server-with-tls.com>:25671",
    //"RabbitMqRootUri": "amqp://<your-server.com>:25672",
    "ServerCertCommonName": "server-CN-from-cert",
    "ClientCertPath": "Properties/certificates/client_certificate.pem",
    "ClientCertPassword": "some-cert-password",
    "UserName": "some-user",
    "Password": "some-password"
  }
}